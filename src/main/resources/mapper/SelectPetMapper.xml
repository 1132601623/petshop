<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xcy.petshop.mapper.SelectPetMapper">
    <select id="selectAllCatById" parameterType="int" resultType="Cat">
        select * from pet where p_id = #{id}
    </select>
    <select id="selectAllDogById" parameterType="int" resultType="Dog">
        select * from pet where p_id = #{id}
    </select>
</mapper>